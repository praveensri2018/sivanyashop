},{
  "name": "Get Product By Id",
  "request": {
    "method": "GET",
    "header": [
      { "key": "Authorization", "value": "Bearer {{adminToken}}" }
    ],
    "url": {
      "raw": "http://localhost:3000/api/products/{{productId}}",
      "protocol": "http",
      "host": ["localhost"],
      "port": "3000",
      "path": ["api", "products", "{{productId}}"]
    }
  },
  "event": [
    {
      "listen": "test",
      "script": {
        "exec": [
          "/* Postman tests for GET /api/products/:id - validates shape used by frontend */",
          "pm.test('Status code is 200', function () {",
          "  pm.response.to.have.status(200);",
          "});",
          "",
          "let body;",
          "pm.test('Response is valid JSON', function() {",
          "  body = pm.response.json();",
          "  pm.expect(body).to.be.an('object');",
          "});",
          "",
          "pm.test('Response contains product object', function() {",
          "  pm.expect(body).to.have.property('product');",
          "  pm.expect(body.product).to.be.an('object');",
          "});",
          "",
          "pm.test('Product required fields exist and types are correct', function() {",
          "  const p = body.product;",
          "  pm.expect(p).to.have.property('Id').that.is.a('number');",
          "  pm.expect(p).to.have.property('Name').that.is.a('string');",
          "  pm.expect(p).to.have.property('Description');",
          "  pm.expect(p).to.have.property('CategoryIds');",
          "  pm.expect(Array.isArray(p.CategoryIds)).to.be.true;",
          "});",
          "",
          "pm.test('Images array shape (if present) matches expectations', function() {",
          "  const p = body.product;",
          "  pm.expect(p).to.have.property('images');",
          "  pm.expect(Array.isArray(p.images)).to.be.true;",
          "  if (p.images.length > 0) {",
          "    const img = p.images[0];",
          "    pm.expect(img).to.have.property('Id');",
          "    pm.expect(img).to.have.property('ImageUrl').that.is.a('string');",
          "  }",
          "});",
          "",
          "pm.test('Variants array shape (if present) matches expectations', function() {",
          "  const p = body.product;",
          "  pm.expect(p).to.have.property('variants');",
          "  pm.expect(Array.isArray(p.variants)).to.be.true;",
          "  if (p.variants.length > 0) {",
          "    const v = p.variants[0];",
          "    pm.expect(v).to.have.property('Id');",
          "    pm.expect(v).to.have.property('SKU');",
          "    pm.expect(v).to.have.property('VariantName');",
          "    pm.expect(v).to.have.property('StockQty');",
          "    // prices may be present as `prices` array or `customerPrice`/`retailerPrice`",
          "    pm.expect(v).to.satisfy((obj) => {",
          "      return (Array.isArray(obj.prices) || obj.hasOwnProperty('customerPrice') || obj.hasOwnProperty('retailerPrice'))",
          "    }, 'variant must have prices array or customerPrice/retailerPrice fields');",
          "  }",
          "});",
          "",
          "pm.test('Optional convenience fields', function() {",
          "  const p = body.product;",
          "  // ImagePath and IsActive/IsFeatured may exist",
          "  if (p.hasOwnProperty('ImagePath')) pm.expect(p.ImagePath === null || typeof p.ImagePath === 'string');",
          "  if (p.hasOwnProperty('IsActive')) pm.expect(typeof p.IsActive === 'number' || typeof p.IsActive === 'boolean');",
          "});"
        ],
        "type": "text/javascript"
      }
    },  {
    "name": "Update Variant (PUT)",
    "request": {
      "method": "PUT",
