        "raw": "{\n  \"sku\": \"XYZ-BLK-128-upd\",\n  \"variantName\": \"Black 128GB Updated\",\n  \"attributes\": \"Color:Black;Storage:128GB\",\n  \"stockQty\": 120\n}"
      },
      "url": {
        "raw": "http://localhost:3000/api/products/variant/{{variantId}}",
        "protocol": "http",
        "host": ["localhost"],
        "port": "3000",
        "path": ["api", "products", "variant", "{{variantId}}"]
      }
    },
    "event": [
      {
        "listen": "test",
        "script": {
          "exec": [
            "pm.test('Status is 200 or 201', function(){ pm.response.to.have.status.oneOf([200,201]); });",
            "let body = pm.response.json();",
            "pm.test('Response contains success true or variant object', function(){",
            "  pm.expect(body).to.satisfy(b => b.success === true || b.variant || (typeof b === 'object'));",
            "});",
            "pm.test('Variant returned has Id and SKU', function(){",
            "  const variant = body.variant ?? body;",
            "  pm.expect(variant).to.have.property('Id');",
            "  pm.expect(variant).to.have.property('SKU');",
            "  pm.expect(variant.SKU).to.be.a('string');",
            "});"
          ],
          "type": "text/javascript"
        }
      }
    ],
